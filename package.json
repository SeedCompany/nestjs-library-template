{
  "name": "@seedcompany/nestjs-template-library",
  "version": "0.0.1",
  "description": "",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint --ext .ts,.tsx --fix --max-warnings 0 .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "type-check": "tsc -p tsconfig.check.json"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt-cli": "^11.0.0",
    "@nestjs/cli": "^7.5.4",
    "@nestjs/common": "^7.6.4",
    "@nestjs/core": "^7.6.4",
    "@nestjs/schematics": "^7.2.5",
    "@nestjs/testing": "^7.6.4",
    "@seedcompany/eslint-plugin": "^1.3.5",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "eslint": "^7.16.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.6"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seedcompany/nestjs-template-library"
  },
  "publishConfig": {
    "access": "public"
  }
}
